apiVersion: v1
data:
  local_config.yaml: |
    server:
      gatewayServer:
        url: http://ks-apigateway.kubesphere-system.svc
        wsUrl: ws://ks-apigateway.kubesphere-system.svc

      extendApiServer:
        wsUrl: ws://ks-apiserver.kubesphere-system.svc
        
      openpitrixServer:
        wsUrl: ws://openpitrix-api-gateway.openpitrix-system.svc:9100

      http:
        hostname: localhost
        port: 8000
        static:
          production:
            /public: server/public
            /assets: dist/assets
            /dist: dist
      redis:
        port: 6379
        host: redis.kubesphere-system.svc
      sessionTimeout: 7200000 
    client:
      version:
        kubesphere: {{ ks_version }}
        kubernetes: {{ kube_version }}
        openpitrix: {{ openpitrix_version }}
kind: ConfigMap
metadata:
  name: ks-console-config
  namespace: kubesphere-system
